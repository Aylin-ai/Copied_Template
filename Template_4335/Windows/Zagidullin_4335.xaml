<Window x:Class="Template_4335.Windows.Zagidullin_4335"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template_4335.Windows"
        mc:Ignorable="d"
        Title="Zagidullin_4335" Height="300" Width="500">
    <StackPanel>
        <TextBlock Text="Изменения выполнил студент группы 4335 Загидуллин Айдар"
               HorizontalAlignment="Center" VerticalAlignment="Center"
               FontSize="30" TextWrapping="Wrap" FontStyle="Italic"/>

        <Button Content="Импорт данных" Margin="10"
                Click="Button_Click"/>

        <Button Content="Экспорт в Excel" Margin="10"
                Click="Button_Click_1"/>

        <Button Content="Импорт JSON данных" Margin="10"
                Click="Button_Click_2"/>

        <Button Content="Экспорт в Word" Margin="10"
                Click="Button_Click_3"/>

    </StackPanel>
